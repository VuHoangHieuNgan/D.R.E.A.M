# ========================================
# ENVIRONMENT VARIABLES - CLIENT
# ========================================
# Copy file này thành .env và điền các giá trị thực

# API Backend URL
# Development: http://localhost:3000
# Production: https://your-api-domain.com
VITE_APP_API_URL=http://localhost:3000

# ========================================
# OPTIONAL CONFIGURATIONS
# ========================================

# Game Settings (nếu muốn customize)
# VITE_GAME_DIFFICULTY=normal
# VITE_GAME_TOTAL_DAYS=7
# VITE_ENABLE_GAME_ANALYTICS=false

# Analytics (nếu có)
# VITE_GOOGLE_ANALYTICS_ID=
# VITE_ENABLE_TRACKING=false

# Feature Flags
# VITE_ENABLE_GAME=true
# VITE_ENABLE_CONTACT_FORM=true
